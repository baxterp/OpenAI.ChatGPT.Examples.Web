@{
    var jsonData = ViewBag.jsonData;
}

<div class="container">
    <div class="row justify-content-md-center">
        <div class="col-sm-12 col-md-12 col-lg-8">
            <div class="jumbotron">
                <h2 class="display-5" style="text-align:center">JSON Data to C# Model</h2>
                <p class="lead">This is an example of using ChatGPT to create a C# model from a set of JSON data.</p>
                <hr class="my-4">
                <p>JSON Data</p>
                <textarea class="CEtextarea" id="inputText">@jsonData</textarea>
                <button id="btnSubmit" class="btn btn-primary" style="margin-bottom:5px;" onclick="javascript:GetCSharpModel()">Get C# Model</button>
                <textarea class="CEtextarea" readonly id="outputText"></textarea>
            </div>
        </div>
    </div>
</div>